en(lines_list)  # finds average sentiment\n        \n    result = pd.DataFrame({\"text\" : [text],\n                          \"score\": [sentiment]},\n                            index = [1])\n    return result\n"},{"ValueType":"Literal","LinkedGlobalParameter":null,"Name":"Python Version","Value":"Anaconda 4.0/Python 3.5"}],"IsPartOfPartialRun":false,"InputPortsInternal":[{"Name":"Dataset1","DataSourceId":null,"TrainedModelId":null,"TransformModuleId":null,"Language":"DotNet","Version":"4.5.0","PortIndex":null,"NodeId":"3da54fcc-248d-4260-ad89-c88b2fb33b27-9"},{"Name":"Dataset2","DataSourceId":null,"TrainedModelId":null,"TransformModuleId":null,"Language":"DotNet","Version":"4.5.0","PortIndex":null,"NodeId":"3da54fcc-248d-4260-ad89-c88b2fb33b27-9"},{"Name":"Script Bundle","DataSourceId":null,"TrainedModelId":null,"TransformModuleId":null,"Language":"DotNet","Version":"4.5.0","PortIndex":null,"NodeId":"3da54fcc-248d-4260-ad89-c88b2fb33b27-9"}],"OutputPortsInternal":[{"Name":"Result Dataset","NodeId":"3da54fcc-248d-4260-ad89-c88b2fb33b27-9","OutputType":null},{"Name":"Python Device","NodeId":"3da54fcc-248d-4260-ad89-c88b2fb33b27-9","OutputType":null}]},{"Id":"dfb40907-3a92-4966-8847-65cd0da24810-106","ModuleId":"506153734175476c4f62416c57734963.4fbef0ab2c8e4a25b5c47be76eac33d6.v1-default-1705","Comment":"","CommentCollapsed":true,"Language":"DotNet","Version":"4.5.0","UsePreviousResults":false,"ModuleParameters":[{"ValueType":"Literal","LinkedGlobalParameter":null,"Name":"DataFormat","Value":"CSV"},{"ValueType":"Literal","LinkedGlobalParameter":null,"Name":"HasHeader","Value":"True"},{"ValueType":"Literal","LinkedGlobalParameter":null,"Name":"Data","Value":"Text\r\nRT @GameOfThrones: 2 days. This sucks #GameofThroneshttps://t.co/7slYhtamj2"}],"IsPartOfPartialRun":false,"InputPortsInternal":[],"OutputPortsInternal":[{"Name":"dataset","NodeId":"dfb40907-3a92-4966-8847-65cd0da24810-106","OutputType":null}]}],"SerializedClientData":"<?xml version='1.0' encoding='utf-16'